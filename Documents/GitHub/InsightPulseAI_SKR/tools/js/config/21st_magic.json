  },
  "animation_levels": {
    "subtle": {
      "duration": "0.3s",
      "effects": ["fade", "slide"],
      "transition_time": "0.3s",
      "particle_density": "low",
      "glow_effects": false,
      "motion_blur": false,
      "parallax_depth": 0.1
    },
    "moderate": {
      "duration": "0.5s",
      "effects": ["fade", "slide", "scale", "rotate"],
      "transition_time": "0.5s",
      "particle_density": "medium",
      "glow_effects": true,
      "motion_blur": false,
      "parallax_depth": 0.2
    },
    "immersive": {
      "duration": "0.8s",
      "effects": ["fade", "slide", "scale", "rotate", "blur", "color"],
      "transition_time": "0.8s",
      "particle_density": "high",
      "glow_effects": true,
      "motion_blur": true,
      "parallax_depth": 0.4
    }
  },
  "browser_compatibility": {
    "minimum_versions": {
      "chrome": 80,
      "firefox": 80,
      "safari": 13,
      "edge": 80
    },
    "webgl_required": true,
    "fallback_enabled": true
  },
  "performance_settings": {
    "low": {
      "particles_enabled": false,
      "glow_effects": false,
      "animation_complexity": "minimal",
      "3d_enabled": false,
      "texture_quality": "low",
      "shadow_quality": "off"
    },
    "medium": {
      "particles_enabled": true,
      "glow_effects": false,
      "animation_complexity": "moderate",
      "3d_enabled": true,
      "texture_quality": "medium",
      "shadow_quality": "low"
    },
    "high": {
      "particles_enabled": true,
      "glow_effects": true,
      "animation_complexity": "full",
      "3d_enabled": true,
      "texture_quality": "high",
      "shadow_quality": "medium"
    },
    "ultra": {
      "particles_enabled": true,
      "glow_effects": true,
      "animation_complexity": "full",
      "3d_enabled": true,
      "texture_quality": "ultra",
      "shadow_quality": "high"
    }
  },
  "visualization_types": {
    "3d_bar": {
      "enabled": true,
      "complexity": "medium",
      "fallback": "bar"
    },
    "3d_pie": {
      "enabled": true,
      "complexity": "medium",
      "fallback": "doughnut"
    },
    "3d_line": {
      "enabled": true,
      "complexity": "high",
      "fallback": "line"
    },
    "3d_scatter": {
      "enabled": true,
      "complexity": "high",
      "fallback": "scatter"
    },
    "particle_flow": {
      "enabled": true,
      "complexity": "ultra",
      "fallback": "line"
    },
    "heat_surface": {
      "enabled": true,
      "complexity": "high",
      "fallback": "heatmap"
    }
  },
  "cdn_resources": {
    "three_js": "https://cdn.jsdelivr.net/npm/three@0.137.0/build/three.min.js",
    "gsap": "https://cdn.jsdelivr.net/npm/gsap@3.9.1/dist/gsap.min.js",
