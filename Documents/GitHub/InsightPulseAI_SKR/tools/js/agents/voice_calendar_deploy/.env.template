# Voice Calendar Agent - Environment Variables
# Copy to .env and fill in your keys

# Google Calendar API (OAuth2)
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8000/oauth2callback

# ElevenLabs TTS API
ELEVEN_API_KEY=your_elevenlabs_api_key_here

# Claude API (if using directly instead of through Pulser)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Echo STT Configuration
ECHO_MODEL_PATH=/path/to/whisper/models
ECHO_LOG_LEVEL=info

# Caca QA Settings
CACA_VALIDATION_LEVEL=standard
CACA_ERROR_THRESHOLD=0.8