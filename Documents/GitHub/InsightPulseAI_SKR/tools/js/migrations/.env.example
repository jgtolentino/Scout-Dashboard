# Azure SQL Database Migration Configuration
# Copy this file to .env and fill in your values

# Database Configuration
SQL_ADMIN_PASSWORD="your_sql_admin_password_here"
SERVER_NAME="sqltbwaprojectscoutserver"
DATABASE_NAME="SQL-TBWA-ProjectScout-Reporting-Prod"
RESOURCE_GROUP="RG-TBWA-ProjectScout-Compute"

# Optional: Storage account for backups
STORAGE_ACCESS_KEY="your_storage_access_key_here"
STORAGE_ACCOUNT="projectscoutdata"

# Migration Settings
DRY_RUN=false
BACKUP_BEFORE_MIGRATION=true
VALIDATE_AFTER_MIGRATION=true

# Notification Settings (optional)
TEAMS_WEBHOOK_URL=""
EMAIL_NOTIFICATION=""