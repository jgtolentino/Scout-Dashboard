{
  "name": "scout-dashboard-web",
  "version": "5.0.0",
  "private": true,
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.3",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.51.1",
    "@types/react-syntax-highlighter": "15.5.13",
    "clsx": "^2.1.1",
    "echarts": "^6.0.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.396.0",
    "mssql": "11.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-markdown": "10.1.0",
    "react-router-dom": "^6.23.1",
    "react-syntax-highlighter": "15.6.6",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "uuid": "^9.0.1",
    "zod": "4.0.17"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:analyze": "vite build && npx vite-bundle-analyzer build/stats.html",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "jest --config jest.config.js",
    "test:e2e": "playwright test tests/e2e/iot",
    "test:perf": "playwright test tests/performance/iot",
    "test:sec": "playwright test tests/security/iot",
    "test:a11y": "playwright test tests/accessibility/iot",
    "test:reliability": "playwright test tests/reliability/iot",
    "test:monitoring": "playwright test tests/monitoring/iot",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:perf && npm run test:sec && npm run test:a11y && npm run test:reliability && npm run test:monitoring",
    "validate:full": "python3 -m venv .venv && . .venv/bin/activate && pip install -r validators/requirements.txt >/dev/null && python validators/validate_scout_v6.py",
    "validate:mcp": "SUPABASE_MCP_ALLOW_WRITES=true python3 validators/validate_scout_v6_mcp.py",
    "smoke:edges": "./scripts/smoke-tests.sh edges",
    "smoke:api": "./scripts/smoke-tests.sh api",
    "smoke:csv": "./scripts/smoke-tests.sh csv",
    "smoke:all": "./scripts/smoke-tests.sh all",
    "pw:install": "playwright install --with-deps",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "lighthouse": "lighthouse http://localhost:5173 --chrome-flags=\"--headless\" --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "lighthouse:server": "lhci server --config=../../.lighthouserc.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.2",
    "@jest/globals": "^29.7.0",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.47.0",
    "@sentry/nextjs": "^7.99.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "critters": "0.0.23",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lighthouse": "^11.4.0",
    "playwright": "1.55.0",
    "postcss": "^8.4.38",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.4",
    "terser": "5.43.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2",
    "vite": "^5.3.1",
    "vite-bundle-analyzer": "^0.10.6",
    "web-vitals": "^3.5.0"
  }
}
